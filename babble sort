#include <stdio.h>
int A[8]={9,3,7,1,5,6,2,4};
int n=8;
void swap(int p,int q)
{
    int t;
    t=A[p];
    A[p]=A[q];
    A[q]=t;
}
int show()
{
int k;
for(k=0;k<=n-1;k++)
printf("%d\t",A[k]);
printf("\n");

}
int main() {
    int i,j;
    show();
    //babble sort
    for(i=0;i<n-1;i++){
        for(j=i+1;j<n-i-1;j++){
            if(A[i]>A[j+1])
            swap(i,j+1);
        }
        
    }
    show();
    return 0;
}
