#include <iostream>
    using namespace std;
    int gcd (int a,int b,int c)
    {
        if(b==0 & c==0)
        return a;
        return gcd(b,a%b,c%b);
        
    }
    int main() {
        int a=36,b=60,c=87;
        int result=gcd(a,c,b);
        cout<<"GCD of a , b &c is "<<result<<endl;
        return 0;
    }
